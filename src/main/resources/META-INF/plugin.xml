<idea-plugin>
    <id>pl.dominikw.Windchill-Intellij-Plugin</id>
    <name>Windchill Plugin</name>
    <vendor email="doominikwoojciechowski@gmail.com">Dominik Wojciechowski</vendor>
    <idea-version since-build="191"/>

    <application-components>
        <component>
            <implementation-class>pl.dominikw.Windchill</implementation-class>
        </component>
    </application-components>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="Windchill" anchor="right" factoryClass="pl.dominikw.ui.MainSubWindowFactory"/>
        <projectService serviceImplementation="pl.dominikw.configuration.PluginConfiguration"/>
        <applicationService serviceInterface="pl.dominikw.service.HttpService" serviceImplementation="pl.dominikw.service.impl.HttpServiceImpl"/>
        <projectService serviceInterface="pl.dominikw.service.WncConnectorService"
                        serviceImplementation="pl.dominikw.service.impl.WncConnectorServiceImpl"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>